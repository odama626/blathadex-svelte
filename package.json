{
  "name": "blathadex-svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@egjs/svelte-infinitegrid": "^4.1.1",
    "@poppanator/sveltekit-svg": "^0.2.2",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "long-press-event": "^2.4.4",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "svelte-sequential-preprocessor": "^0.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "dependencies": {
    "@analytics/google-analytics": "^0.5.3",
    "@prisma/client": "^3.7.0",
    "@sendgrid/client": "^7.6.0",
    "@sendgrid/mail": "^7.6.0",
    "@sveltejs/adapter-node": "^1.0.0-next.58",
    "analytics": "^0.7.19",
    "cookie": "^0.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.debounce": "^4.0.8",
    "lodash.orderby": "^4.6.0",
    "prisma": "^3.7.0",
    "sass": "^1.45.1",
    "sqlite3": "^5.0.2",
    "svelte-image": "^0.2.9"
  }
}
